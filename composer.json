{
  "name": "ice-php/mongo",
  "description": "对MongoDB进行封装",
  "keywords": [
    "icePHP",
    "mongo"
  ],
  "homepage": "https://github.com/ice-php/mongo",
  "type": "composer",
  "license": "MIT",
  "support": {
    "source": "https://github.com/ice-php/mongo"
  },
  "authors": [
    {
      "name": "blueice",
      "email": "liyong@nenu.edu.cn"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ice-php/config": "dev-master",
    "ice-php/debug": "dev-master",
    "ice-php/functions": "dev-master"
  },
  "minimum-stability": "dev",
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "icePHP\\": "src"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  },
  "config": {
    "process-timeout": 1800
  }
}
